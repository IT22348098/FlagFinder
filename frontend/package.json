    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@testing-library)/)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/build/"
    ]
  }
